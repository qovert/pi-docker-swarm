{
  "id": null,
  "title": "Pi Docker Swarm Basic",
  "tags": ["docker", "swarm", "raspberry-pi"],
  "timezone": "browser",
  "editable": true,
  "panels": [
    {
      "id": 1,
      "title": "Node Exporter Status",
      "type": "stat",
      "targets": [
        {
          "expr": "up{job=\"node-exporter\"}",
          "legendFormat": "{{instance}}"
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 24,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "CPU Usage %",
      "type": "timeseries",
      "targets": [
        {
          "expr": "100 - (avg(rate(node_cpu_seconds_total{mode=\"idle\"}[5m])) by (instance) * 100)",
          "legendFormat": "{{instance}}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 6
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "30s",
  "schemaVersion": 37,
  "version": 1
}
